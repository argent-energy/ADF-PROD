{
	"name": "Sharepoint_StdCostsSpread_Data",
	"properties": {
		"description": "Link to Sharepoint site \"PowerBI data\" folder \"Finance - Standard Costs and Spread\"",
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "abc"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "https://argentenergyltd.sharepoint.com/sites/PowerBIdata/_api/web/GetFileByServerRelativeUrl('@{linkedService().FileName}')/$value",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}