{
	"name": "Copy_Oracle_to_Blob_OO",
	"properties": {
		"activities": [
			{
				"name": "Copy Oracle files from FTP server to blob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "FtpReadSettings",
							"recursive": true,
							"useBinaryTransfer": true,
							"disableChunking": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "OracleDeltaSource_OO",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Oracle Deltas Test"
		},
		"annotations": []
	}
}