			{
				"name": "Delete previous month data",
				"description": "Delete all rows in [RAW].[F.ActTankItem] for the previous month from today.",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SqlServerArgent",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "NonQuery",
							"text": {
								"value": "DELETE FROM [RAW].[F.ActTankItem]\nWHERE [MONTH] = DATEPART(m, DATEADD(MM, -2, GETDATE()))\n    AND [YEAR] = DATEPART(year, DATEADD(MM, -2, GETDATE()))",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Delete null rows",
				"description": "Delete all rows where the Unique Ref is null.",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Copy Actual Tank and Item sheet",
						"dependencyConditions": [
							"Succeeded"
						]
