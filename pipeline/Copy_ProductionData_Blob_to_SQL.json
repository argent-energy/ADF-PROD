					"referenceName": "SqlServerArgent",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": "/* Truncate BDR table */\r\nTRUNCATE TABLE [RAW].[F.Production_StanlowBDR_2024];\r\n\r\n/* Drop [ID] from BDR table\r\nALTER TABLE [RAW].[F.Production_StanlowBDR_2024]\r\nIF EXISTIS DROP COLUMN [ID];*/\r\n\r\n/* Truncate PTP table */\r\nTRUNCATE TABLE [RAW].[F.Production_StanlowPTP_2024];\r\n\r\n/* Drop [ID] from PTP table\r\nALTER TABLE [RAW].[F.Production_StanlowPTP_2024]\r\nDROP COLUMN [ID];*/"
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			},
			{
				"name": "Add row id and datestamp to SBDR",
				"type": "Script",
				"dependsOn": [
					{
						"activity": "Copy SBDR Production sheet",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
